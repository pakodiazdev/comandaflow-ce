{
    "name": "cf-ce/auth",
    "description": "CF Auth Package - Centralized authentication and user management with Laravel Passport and Spatie Laravel Permission",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ComandaFlow Team",
            "email": "dev@comandaflow.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/passport": "^12.0",
        "spatie/laravel-permission": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "CF\\CE\\Auth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CF\\CE\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
