{
    "name": "cf-ce/hello-world",
    "description": "A simple Hello World package for CF",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CF Team",
            "email": "dev@comandaflow.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "CF\\HelloWorld\\": "src/"
        }
    },
    "extra": {
